{
  "name": "Notebookify",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Redirect URL containing IPYNB files to Nbviewer.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Open Notebook Viewer",
    "default_popup": "popup.html"
  },
  "permissions" : [
    "tabs"
  ]
}